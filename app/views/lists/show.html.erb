<h3><%= @list.name %> </h3>
<p><%= link_to "Add a Movie", new_list_bookmark_path(@list) %></p>
<p><%= link_to "Home", root_path %></p>
<ol>
   <% @bookmarks.each do |bookmark| %>
   <li><%= bookmark.movie.title %>
    <%= link_to "Delete",
            bookmark_path(bookmark),
            method: :delete,
            data: { confirm: "Are you sure?" } %>
   </li>
   <% end %>
</ol>
